{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:watch": "nodemon --inspect",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "cache:clear": "ts-node ./node_modules/typeorm/cli.js cache:clear",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "schemadownload": "apollo schema:download --endpoint=http://localhost:3030/graphql",
    "generate": "ts-node ./src/generate-typings.ts"
  },
  "dependencies": {
    "@nest-modules/mailer": "^1.3.22",
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.4.4",
    "@nestjs/graphql": "^7.7.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "7.4.4",
    "@nestjs/typeorm": "^7.1.4",
    "@turf/turf": "^5.1.6",
    "@types/moment-timezone": "^0.5.30",
    "@types/node-schedule": "^1.3.1",
    "@yops/nest-mailer": "^0.1.21",
    "africastalking": "^0.5.2",
    "apollo-server": "^2.18.2",
    "apollo-server-express": "^2.18.2",
    "base64-img": "^1.0.4",
    "bcryptjs": "^2.4.3",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "convert-excel-to-json": "^1.7.0",
    "core-util-is": "^1.0.2",
    "cors": "^2.8.5",
    "daraja": "^1.0.1",
    "date-fns": "^2.16.1",
    "fast-json-stringify": "^2.2.9",
    "file-system": "^2.2.2",
    "graphql": "^15.3.0",
    "graphql-subscriptions": "1.1.0",
    "graphql-tools": "^6.2.4",
    "graphql-type-json": "^0.3.2",
    "jwt-decode": "^3.0.0",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31",
    "mpesa-api": "^2.3.3",
    "mysql": "^2.18.1",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.14",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "shortid": "^2.2.16",
    "soap": "^0.33.0",
    "typeorm": "^0.2.28",
    "unique-random": "^2.1.0",
    "wkx": "^0.5.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@graphql-codegen/cli": "1.18.0",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "7.1.2",
    "@nestjs/testing": "7.4.4",
    "@types/express": "4.17.8",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.2",
    "@types/supertest": "2.0.10",
    "jest": "26.6.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "supertest": "5.0.0",
    "ts-jest": "26.4.1",
    "ts-loader": "8.0.6",
    "ts-node": "^9.0.0",
    "tsc-watch": "4.2.9",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.3",
    "typescript": "^4.0.3"
  }
}
